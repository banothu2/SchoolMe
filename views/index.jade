extends layout

block append head 
	link(rel='stylesheet', href='/stylesheets/main.css')
	script(src='https://cdn.firebase.com/js/client/1.0.6/firebase.js')
	script(src='https://cdn.firebase.com/js/simple-login/1.3.2/firebase-simple-login.js')
	script(src='/javascripts/snap.svg-min.js')
	script(src='/javascripts/hovers.js')
	script(src='/javascripts/Chart.js')

block content
	#myModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
		.modal-dialog
			.modal-content
				.modal-header
					button.close(type='button', data-dismiss='modal', aria-hidden='true')
					h4#modalTitle.modal-title Modal title
				.modal-body
					.col-md-6.col-lg-6
						#chart
							canvas(id="myChart" width="200" height="200")
						img(src="/images/key.png")
					.col-md-5.col-lg-5
						#tagsArea
				.modal-footer
					button.btn.btn-default(type='button', data-dismiss='modal') Close
					button.btn.btn-primary(type='button') Save changes


	.container
		.row
			canvas.center-block(id="myChart2" width="1050" height="300")
		section.grid.clearfix#grid

		
